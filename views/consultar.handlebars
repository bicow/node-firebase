<div class="container text-center">
    <div class="row">
        <div class="col-12">
            <nav class="navbar bg-primary navbar-expand-lg" data-bs-theme="dark">
                <div class="container-fluid">
                    <a class="navbar-brand" href="#">SISTEMA WEB</a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false"
                        aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                        <div class="navbar-nav">
                            <a class="nav-link active" aria-current="page" href="/">Cadastrar</a>
                            <a class="nav-link" href="/consulta">Consultar</a>
                        </div>
                    </div>
                </div>
            </nav>
        </div>
        <div class="col-1">
            &nbsp;
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <p class="text-start fs-5">Cadastrar - Agendamento de Potenciais Clientes</p>
            <p class="text-start fs-6">Sistema Utilizado para agendamento de serviços.</p>
            <div class="col-1">
                &nbsp;
            </div>
            <table class="table table-bordered">
                <thead class="text-light bg-primary">
                    <tr>
                        <th>Nome</th>
                        <th>Telefone</th>
                        <th>Origem</th>
                        <th>Data de Contato</th>
                        <th>Observação</th>
                        <th>Ações</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each agendamentos}}
                    <tr>
                        <td>{{this.data.nome}}</td>
                        <td>{{this.data.telefone}}</td>
                        <td>{{this.data.origem}}</td>
                        <td>{{this.data.data_contato}}</td>
                        <td>{{this.data.observacao}}</td>
                        <td>
                            <button type="button" class="btn text-light" style="background-color:dodgerblue;"><a href="/editar/{{this.id}}" style="color:inherit">Editar</a></button>
                            <button type="button" class="btn text-light" style="background-color: darkred;"><a href="/excluir/{{this.id}}"
                                    style="color:inherit">Excluir</a></button>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>